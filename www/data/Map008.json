{"id":1,"name":"EV001","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":221,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["                                       \\{Kabanata 2"]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["                           \\{Ang Binatang Nakagapos"]},{"code":222,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,1,1]},{"code":401,"indent":0,"parameters":["                         Sa may gitna nitong mapanglaw na gubat,\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         may punong Higerang daho'y kulay pupas;\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         dito nakagapos ang kahabag-habag,\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         isang pinag-usig ng masamang palad.\\.\\.\\."]},{"code":101,"indent":0,"parameters":["",0,1,1]},{"code":401,"indent":0,"parameters":["                         Bagun-taong basal na ang anyo't tindig,\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         kahit natatali kamay, paa't liig,\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         kundi si Narsiso'y tunay na Adonis,\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         mukha'y sumisilang sa gitna ng sakit.\\.\\.\\."]},{"code":101,"indent":0,"parameters":["",0,1,1]},{"code":401,"indent":0,"parameters":["                         Makinis ang balat at anaki burok,\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         pilikmata't kilay mistulang balantok;\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         bagong sapong ginto ang kulay ng buhok,\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         sangkap ng katawa'y pawang magkaayos.\\.\\.\\."]},{"code":101,"indent":0,"parameters":["",0,1,1]},{"code":401,"indent":0,"parameters":["                         Dangan doo'y walang Oreadang Ninfas,\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         gubat na palasyo ng masidhing Harp'yas,\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         nangaawa disi't naakay lumiyag\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         sa himalang tipon ng karikta't hirap.\\.\\.\\."]},{"code":101,"indent":0,"parameters":["",0,1,1]},{"code":401,"indent":0,"parameters":["                         Ang abang uyamin ng dalita't sakit â€”\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         ang dalawang mata'y bukal ang kaparis;\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         sa luhang nanatak at tinangis-tangis,\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         ganito'y damdamin ng may awang dibdib.\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Mahiganting langit! bangis mo'y nasaan?\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ngayo'y naniniig sa pagkagulaylay;\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"bago'y ang bandila ng lalong kasam-an\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"sa Reynong Albanya'y iniwawagayway.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Sa loob at labas ng bayan kong sawi,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"kaliluha'y siyang nangyayaring hari,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"kagalinga't bait ay nalulugami,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ininis sa hukay ng dusa't pighati.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Ang magandang asal ay ipinupukol\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"sa laot ng dagat kutya't linggatong;\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"balang magagaling ay ibinabaon\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"at inililibing na walang kabaong.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Nguni, at ang lilo't masasamang loob\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"sa trono ng puri ay iniluluklok,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"at sa balang sukab na may asal-hayop,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"mabangong insenso ang isinusuob.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Kaliluha't sama ang ulo'y nagtayo\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"at ang kabaita'y kimi at nakayuko;\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"santong katuwira'y lugami at hapo,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ang luha na lamang ang pinatutulo.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"At ang balang bibig na binubukalan\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ng sabing magaling at katotohanan,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"agad binibiyak at sinisikangan\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ng kalis ng lalong dustang kamatayan.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"O, taksil na pita sa yama't mataas!\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"O, hangad sa puring hanging lumilipas!\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ikaw ang dahilan ng kasam-ang lahat\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"at niring nasapit na kahabag-habag!\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Sa korona dahil ng Haring Linceo,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"at sa kayamanan ng dukeng ama ko,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ang ipinangahas ng Konde Adolfo\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"sabugan ng sama ang Albanyang Reyno.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Ang lahat ng ito, maawaing Langit,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"iyong tinutungha'y ano't natitiis?\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"mula ka ng buong katuwira't bait,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"pinayagang mong ilubog ng lupit.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Makapangyarihang kamay mo'y ikilos,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"papamilantikin ang kalis ng poot;\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"sa Reynong Albanya'y kusang ibulusok\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ang Iyong higanti sa masamang-loob.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Bakit kalangita'y bingi ka sa akin?\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ang tapat kong luhog ay hindi mo dinggin?\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"'di yata't sa isang alipusta't iring\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"sampung tainga mo'y ipinangunguling?\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Datapuwa't sino ang tatarok kaya\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"sa mahal mong lihim, Diyos na dakila?\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"walang mangyayari sa balat ng lupa,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"'di may kagaligang iyong ninanasa.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["                         Isang binatang nakagapos sa isang puno ng\\.\\.\\. "]},{"code":401,"indent":0,"parameters":["                         higera sa gitna ng malawak na gubat sa labas\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         ng kahariang Albanya.\\.\\.\\. "]},{"code":401,"indent":0,"parameters":["           "]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["                         Ang binatang ito ay si Florante.\\.\\.\\. "]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["                         Tiyak na kamatayan ang kinabibingitan ng\\.\\.\\. "]},{"code":401,"indent":0,"parameters":["                         kanyang buhay sa dahilang hindi niya\\.\\.\\."]},{"code":401,"indent":0,"parameters":["                         maipagtanggol ang kanyang sarili.\\.\\.\\."]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["                         Sa Albanya mayroong masasamang-loob ang\\.\\.\\.  "]},{"code":401,"indent":0,"parameters":["                         iniluklok sa trono. Dahil sa pag-iimbot ni\\.\\.\\.  "]},{"code":401,"indent":0,"parameters":["                         Konde Adolfo sa korona ni Haring Linseo\\.\\.\\. "]},{"code":401,"indent":0,"parameters":["          "]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["                         at sa kayamanan ng ama ni Florante na si "]},{"code":401,"indent":0,"parameters":["                         Duke Briseo,\\.\\.\\. "]},{"code":401,"indent":0,"parameters":["                         sinabugan niya ng kasamaan ang kaharian.\\.\\.\\."]},{"code":121,"indent":0,"parameters":[4,4,0]},{"code":221,"indent":0,"parameters":[]},{"code":211,"indent":0,"parameters":[1]},{"code":201,"indent":0,"parameters":[0,1,5,5,0,0]},{"code":222,"indent":0,"parameters":[]},{"code":223,"indent":0,"parameters":[[0,0,0,0],60,true]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true}],"x":7,"y":6}
