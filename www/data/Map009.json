{"id":1,"name":"EV001","note":"","pages":[{"conditions":{"actorId":1,"actorValid":false,"itemId":1,"itemValid":false,"selfSwitchCh":"A","selfSwitchValid":false,"switch1Id":1,"switch1Valid":false,"switch2Id":1,"switch2Valid":false,"variableId":1,"variableValid":false,"variableValue":0},"directionFix":false,"image":{"characterIndex":0,"characterName":"","direction":2,"pattern":0,"tileId":0},"list":[{"code":221,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["                                       \\{Kabanata 3"]},{"code":101,"indent":0,"parameters":["",0,2,1]},{"code":401,"indent":0,"parameters":["                                     \\{Alaala ni Laura"]},{"code":222,"indent":0,"parameters":[]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Ay, 'di saan ngayon ako mangangapit!\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"saan ipupukol ang tinangis-tangis,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"kung ayaw na ngayong dinigin ng Langit,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ang sigaw ng aking malumbay na boses!\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Kung siya mong ibig na ako'y magdusa,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Langit na mataas, aking mababata;\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"isagi mo lamang sa puso ni Laura\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ako'y minsan-minsang mapag-alaala.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"At dito sa laot ng dusa't hinagpis,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"malawak na lubhang aking tinatawid;\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"gunita ni Laura sa naabang ibig,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"siya ko na lamang ligaya sa dibdib.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Munting gunam-gunam ng sinta ko't mutya\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"nang dahil sa aki'y dakila kong tuwa;\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"higit sa malaking hirap at dalita,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"parusa ng taong lilo't walang awa.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Sa pagkagapos ko'y guni-gunihin,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"malamig nang bangkay akong nahihimbing;\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"at tinatangisan ng sula ko't giliw,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ang pagkabuhay ko'y walang hangga mandin.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Kung apuhapin ko sa sariling isip,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ang suyuan naman ng pili kong ibig;\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ang pagluha niya kung ako'y may hapis,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"nagiging ligaya yaring madlang sakit.\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Nguni, sa aba ko! sawing kapalaran!\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ano pang halaga ng gayong suyuan\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"kung ang sing-ibig ko'y sa katahimikan\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"ay humihilig na sa ibang kandungan?\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"Sa sinapupunan ng Konde Adolfo,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"aking natatanaw si Laurang sinta ko;\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"kamataya'y nahan ang dating bangis mo,\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["Actor1",0,0,2]},{"code":401,"indent":0,"parameters":["\\{\"nang 'di ko damdamin ang hirap na ito?\"\\.\\.\\."]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["                         Sinabi ni Florante sa kanyang sarili na "]},{"code":401,"indent":0,"parameters":["                         kaya niyang tiisin ang pagdurusa, kung ito "]},{"code":401,"indent":0,"parameters":["                         ang gustong mangyari ng Maykapal\\.\\.\\."]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["                         Iisa lamang ang tangi niyang hiling,\\.\\.\\. "]},{"code":401,"indent":0,"parameters":["                         ang maalala siya ng kanyang minamahal sa\\.\\.\\. "]},{"code":401,"indent":0,"parameters":["                         si Laura.\\.\\.\\."]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["                         Di niya maiwasang maalala ang kanilang "]},{"code":401,"indent":0,"parameters":["                         suyuan, at iniisip niya na baka agawin si "]},{"code":401,"indent":0,"parameters":["                         Laura ng kanyang karibal na si Adolfo.\\.\\.\\."]},{"code":101,"indent":0,"parameters":["",0,1,2]},{"code":401,"indent":0,"parameters":["                         Sa Albanya mayroong masasamang-loob ang\\.\\.\\.  "]},{"code":401,"indent":0,"parameters":["                         iniluklok sa trono. Dahil sa pag-iimbot ni\\.\\.\\.  "]},{"code":401,"indent":0,"parameters":["                         Konde Adolfo sa korona ni Haring Linseo\\.\\.\\. "]},{"code":401,"indent":0,"parameters":["          "]},{"code":221,"indent":0,"parameters":[]},{"code":121,"indent":0,"parameters":[5,5,0]},{"code":211,"indent":0,"parameters":[1]},{"code":201,"indent":0,"parameters":[0,1,8,5,0,0]},{"code":222,"indent":0,"parameters":[]},{"code":223,"indent":0,"parameters":[[0,0,0,0],60,true]},{"code":0,"indent":0,"parameters":[]}],"moveFrequency":3,"moveRoute":{"list":[{"code":0,"parameters":[]}],"repeat":true,"skippable":false,"wait":false},"moveSpeed":3,"moveType":0,"priorityType":0,"stepAnime":false,"through":false,"trigger":3,"walkAnime":true}],"x":7,"y":6}
